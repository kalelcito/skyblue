    {% extends '@Admin/base.html.twig' %}
{% block navegacion %}
{{ include('@Admin/partials/navegacion.html.twig',{'menu':'SkyBlue','submenu':'Mapas'}) }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}

{% endblock %}

{% block javascripts %}
    {{ parent() }}
<script src="{{ asset('admin/js/jasny-bootstrap.js') }}"></script>
{% endblock %}

    {% block body %}
        <style>
            .map {
                width: 100%;
                height: 400px;
            }
        </style>
        <div class="row">
            <div class="col-md-12">
                <div class="white-box">
                    <h3 class="box-title m-b-0">Ver Registro</h3>                <form class="form-horizontal" role="form">                        <div class="form-group">
                            <label class="col-sm-2 control-label">Id</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ mapsDistribuidor.id }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Mapa Google Maps</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                 <iframe src="{{ mapsDistribuidor.url }}" class="map"></iframe>                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Nombre</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ mapsDistribuidor.nombre }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Zona</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ mapsDistribuidor.zona }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Activo</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ mapsDistribuidor.activo|sino }}                     </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Slug</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ mapsDistribuidor.slug }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Metakeys</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ mapsDistribuidor.metakeys }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Metadesc</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ mapsDistribuidor.metadesc }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Creado</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {% if mapsDistribuidor.createdat %}{{ mapsDistribuidor.createdat|date('Y-m-d') }}{% endif %}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Actualizado</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {% if mapsDistribuidor.updatedat %}{{ mapsDistribuidor.updatedat|date('Y-m-d') }}{% endif %}                                </p>
                            </div>
                        </div>                    <div class="form-group m-b-0">
                            <div class="col-sm-12">
                                <a class="btn btn-inverse waves-effect waves-light m-t-10" style="float:left; margin:5px;" href="{{ path('mapsdistribuidor_index') }}">Regresar</a>            <a class="btn btn-info waves-effect waves-light m-t-10" style="float:left;  margin:5px;"  href="{{ path('mapsdistribuidor_edit', { 'id': mapsDistribuidor.id }) }}">Editar</a></div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    {% endblock %}
